{"manifest_version": 3, "name": "FlatNas - 个人导航页", "version": "1.1.0", "description": "轻量级、高度可定制的个人导航页与仪表盘系统 - 浏览器扩展版", "permissions": ["storage", "alarms", "tabs"], "host_permissions": ["http://localhost:*/*", "http://192.168.*/*", "http://10.*/*", "http://172.*/*", "https://*/*"], "background": {"service_worker": "background.js"}, "content_scripts": [{"matches": ["http://*/*", "https://*/*"], "js": ["content.js"], "run_at": "document_idle"}], "action": {"default_popup": "popup.html", "default_icon": {"16": "icons/icon16.png", "48": "icons/icon48.png", "128": "icons/icon128.png"}, "default_title": "FlatNas 快速访问"}, "icons": {"16": "icons/icon16.png", "48": "icons/icon48.png", "128": "icons/icon128.png"}, "options_page": "options.html", "web_accessible_resources": [{"resources": ["assets/*", "icons/*"], "matches": ["<all_urls>"]}]}