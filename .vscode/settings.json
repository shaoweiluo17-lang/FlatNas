{
  "files.associations": {
    "*.vue": "vue",
    "*.css": "css",
    "*.go": "go"
  },
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "go.workspaces": [
    "backend"
  ]
}